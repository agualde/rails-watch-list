<div class="container">
  <div class="row">

    <div class="col-m-4">
    </div>

    <div class="col col-m-4 mt-5">
      <h1>Add a list</h1>

      <%= simple_form_for [@list] do |f| %>
      <%= f.input :name %>
      <%= f.button :submit, "Finished", class: "btn btn-primary mt-3" %>
      <% end %>
      <%= link_to "cancel", lists_path, class: "btn kill-link kill-decoration", id: "cancel-link" %>
    </div>

     <div class="col-m-4">
     </div>

  </div>
</div>
